<?xml version="1.0" encoding="utf8"?><xsl:transform version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:template match="/corporations">		<svg version="1.1"     baseProfile="full"     xmlns="http://www.w3.org/2000/svg"     xmlns:xlink="http://www.w3.org/1999/xlink"     xmlns:ev="http://www.w3.org/2001/xml-events"     width="100%" height="100%">		<for-each select="corp">						<xsl:variable name="points" select="month/@order" />				<polyline fill="none" stroke="{@color}" stroke-width="10" points="{$points}"/>		</for-each>	 </svg></xsl:template></xsl:transform>